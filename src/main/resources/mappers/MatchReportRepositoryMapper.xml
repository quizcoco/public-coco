<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quizcoco.web.repository.MatchReportRepository">


<insert id="save" parameterType="MatchReport">
		INSERT INTO match_report
		(
            user_id,
            wrong,
            quiz_id,
            coco_id,
            enemy_id,
            avatar_id
            
		)
		VALUES
		(
            #{userId},
            #{wrong},
            #{quizId},
            1,1,1
            <!-- #{cocoId},
            #{enemyId},
            #{avatarId} -->
		)
		
	</insert>






</mapper>